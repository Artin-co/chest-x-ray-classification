   image:  artinmajdi/miniconda-cuda-tensorflow:latest

entry_points:
    main:
        parameters:
            epoch: {type: str, default: 3}
            bsize: {type: str, default: 30}
        command: python main.py --epoch {epoch} --bsize {bsize}